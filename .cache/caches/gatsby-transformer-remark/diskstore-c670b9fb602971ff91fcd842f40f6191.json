        }
      },
      {
        "type": "paragraph",
        "children": [
          {
            "type": "text",
            "value": "First, more on CUDA runtime:",
            "position": {
              "start": { "line": 55, "column": 1, "offset": 3765 },
              "end": { "line": 55, "column": 29, "offset": 3793 },
              "indent": []
            }
          }
        ],
