        }
      },
      { "type": "text", "value": "\n" },
      {
        "type": "element",
        "tagName": "p",
        "properties": {},
        "children": [
          {
            "type": "text",
            "value": "All CUDA cards to date use a warp size of 32.",
            "position": {
              "start": { "line": 22, "column": 1, "offset": 1195 },
              "end": { "line": 22, "column": 46, "offset": 1240 }
            }
          }
        ],
        "position": {
          "start": { "line": 22, "column": 1, "offset": 1195 },
          "end": { "line": 22, "column": 46, "offset": 1240 }
        }
      },
      { "type": "text", "value": "\n" },
      {
        "type": "element",
        "tagName": "p",
        "properties": {},
        "children": [
          {
            "type": "text",
            "value": "Each ",
            "position": {
